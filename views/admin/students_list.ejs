<%- include('../partials/admin-header') %>
<body>
  <div class="content">
    <h2>Students List</h2>
    <table class="table">
      <thead>
        <tr>
          <th>PRN</th>
          <th>Name</th>
          <th>Email</th>
          <th>Batch Year</th>
          <th>Department</th>
        </tr>
      </thead>
      <tbody>
        <% students.forEach(student => { %>
          <tr>
            <td><%= student.prn %></td>
            <td><%= student.name %></td>
            <td><%= student.email %></td>
            <td><%= student.batch_year %></td>
            <td><%= student.department %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
  <%- include('../partials/footer') %>
</body>